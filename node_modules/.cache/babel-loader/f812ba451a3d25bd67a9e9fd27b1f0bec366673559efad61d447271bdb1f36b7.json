{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nimport axios from 'axios';\nimport { useRouter } from 'vue-router';\nimport Swal from 'sweetalert2';\nimport { useUserStore } from '@/store/user';\nexport default {\n  setup() {\n    const router = useRouter();\n    const userStore = useUserStore();\n    const username = ref('');\n    const password = ref('');\n    const login = async () => {\n      const payload = {\n        username: username.value,\n        password: password.value\n      };\n      try {\n        const response = await axios.post('http://127.0.0.1:8000/users/login/', payload);\n        if (response.data.status === 'True') {\n          userStore.setUserInfo(response.data.data);\n          router.push('/decis/');\n        } else {\n          Swal.fire({\n            title: '提示',\n            text: response.data.data.logintxt,\n            icon: 'error'\n          });\n        }\n      } catch (error) {\n        Swal.fire({\n          title: '提示',\n          text: '登录失败，请重试！',\n          icon: 'error'\n        });\n      }\n    };\n    const goToRegister = () => {\n      router.push('/register/');\n    };\n    return {\n      username,\n      password,\n      login,\n      goToRegister\n    };\n  }\n};","map":{"version":3,"names":["ref","axios","useRouter","Swal","useUserStore","setup","router","userStore","username","password","login","payload","value","response","post","data","status","setUserInfo","push","fire","title","text","logintxt","icon","error","goToRegister"],"sources":["D:\\Bridge1\\src\\components\\UserLogin.vue"],"sourcesContent":["<template>\r\n    <div class=\"container mt-5\">\r\n        <h2>登录</h2>\r\n        <form @submit.prevent=\"login\">\r\n            <div class=\"mb-3 row\">\r\n                <label for=\"username\" class=\"col-sm-2 col-form-label\">用户名</label>\r\n                <div class=\"col-sm-10\">\r\n                    <input type=\"text\" id=\"username\" v-model=\"username\" class=\"form-control\" placeholder=\"用户名\" required>\r\n                </div>\r\n            </div>\r\n            <div class=\"mb-3 row\">\r\n                <label for=\"password\" class=\"col-sm-2 col-form-label\">密码</label>\r\n                <div class=\"col-sm-10\">\r\n                    <input type=\"password\" id=\"password\" v-model=\"password\" class=\"form-control\" placeholder=\"密码\" required>\r\n                </div>\r\n            </div>\r\n            <p>新用户请先点击注册！</p>\r\n            <div class=\"button-container text-center\">\r\n                <button type=\"submit\" class=\"btn btn-primary me-2\">登录</button>\r\n                <button type=\"button\" class=\"btn btn-secondary\" @click=\"goToRegister\">注册</button>\r\n            </div>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue'; \r\nimport axios from 'axios'; \r\nimport { useRouter } from 'vue-router';\r\nimport Swal from 'sweetalert2'; \r\nimport { useUserStore } from '@/store/user'; \r\n\r\nexport default {\r\n    setup() { \r\n        const router = useRouter();\r\n        const userStore = useUserStore();\r\n        const username = ref('');\r\n        const password = ref('');\r\n\r\n        const login = async () => {\r\n            const payload = { username: username.value, password: password.value };\r\n            try {\r\n                const response = await axios.post('http://127.0.0.1:8000/users/login/', payload);\r\n                if (response.data.status === 'True') {\r\n                    userStore.setUserInfo(response.data.data);\r\n                    router.push('/decis/'); \r\n                } else {\r\n                    Swal.fire({\r\n                        title: '提示',\r\n                        text: response.data.data.logintxt,\r\n                        icon: 'error',\r\n                    });\r\n                }\r\n            } catch (error) {\r\n                Swal.fire({\r\n                    title: '提示',\r\n                    text: '登录失败，请重试！',\r\n                    icon: 'error',\r\n                });\r\n            }\r\n        };\r\n\r\n        const goToRegister = () => {\r\n            router.push('/register/'); \r\n        };\r\n\r\n        return {\r\n            username,\r\n            password,\r\n            login,\r\n            goToRegister,\r\n        };\r\n    },\r\n};\r\n</script>\r\n\r\n<style>\r\nbody {\r\n    font-family: Arial, sans-serif;\r\n    background-color: #f4f4f4;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    height: 100vh;\r\n}\r\n\r\n.container {\r\n    max-width: 400px;\r\n}\r\n</style>\r\n\r\n\r\n\r\n"],"mappings":";AA0BA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,YAAY;AACtC,OAAOC,IAAG,MAAO,aAAa;AAC9B,SAASC,YAAW,QAAS,cAAc;AAE3C,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAMC,MAAK,GAAIJ,SAAS,CAAC,CAAC;IAC1B,MAAMK,SAAQ,GAAIH,YAAY,CAAC,CAAC;IAChC,MAAMI,QAAO,GAAIR,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMS,QAAO,GAAIT,GAAG,CAAC,EAAE,CAAC;IAExB,MAAMU,KAAI,GAAI,MAAAA,CAAA,KAAY;MACtB,MAAMC,OAAM,GAAI;QAAEH,QAAQ,EAAEA,QAAQ,CAACI,KAAK;QAAEH,QAAQ,EAAEA,QAAQ,CAACG;MAAM,CAAC;MACtE,IAAI;QACA,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,oCAAoC,EAAEH,OAAO,CAAC;QAChF,IAAIE,QAAQ,CAACE,IAAI,CAACC,MAAK,KAAM,MAAM,EAAE;UACjCT,SAAS,CAACU,WAAW,CAACJ,QAAQ,CAACE,IAAI,CAACA,IAAI,CAAC;UACzCT,MAAM,CAACY,IAAI,CAAC,SAAS,CAAC;QAC1B,OAAO;UACHf,IAAI,CAACgB,IAAI,CAAC;YACNC,KAAK,EAAE,IAAI;YACXC,IAAI,EAAER,QAAQ,CAACE,IAAI,CAACA,IAAI,CAACO,QAAQ;YACjCC,IAAI,EAAE;UACV,CAAC,CAAC;QACN;MACJ,EAAE,OAAOC,KAAK,EAAE;QACZrB,IAAI,CAACgB,IAAI,CAAC;UACNC,KAAK,EAAE,IAAI;UACXC,IAAI,EAAE,WAAW;UACjBE,IAAI,EAAE;QACV,CAAC,CAAC;MACN;IACJ,CAAC;IAED,MAAME,YAAW,GAAIA,CAAA,KAAM;MACvBnB,MAAM,CAACY,IAAI,CAAC,YAAY,CAAC;IAC7B,CAAC;IAED,OAAO;MACHV,QAAQ;MACRC,QAAQ;MACRC,KAAK;MACLe;IACJ,CAAC;EACL;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}