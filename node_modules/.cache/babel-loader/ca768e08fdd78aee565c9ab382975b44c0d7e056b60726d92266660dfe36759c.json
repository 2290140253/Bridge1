{"ast":null,"code":"import { useUserStore } from '@/store/user';\nexport default {\n  setup() {\n    const userStore = useUserStore();\n    const userInfo = userStore.userInfo;\n    return {\n      userInfo,\n      teamMembers: [{\n        id: 1,\n        position: '总经理',\n        email: '2290140253@qq.com',\n        name: '李清逸',\n        school: '东华大学',\n        class: '信管2201',\n        studentId: '220750204'\n      }\n      // 其他成员\n      ],\n      subMenuVisible: {\n        marketCycle: false,\n        companyData: false,\n        evaluation: false\n      },\n      cyclesToDisplay: []\n    };\n  },\n  methods: {\n    showCycles() {\n      this.subMenuVisible.marketCycle = true;\n    },\n    hideCycles() {\n      this.subMenuVisible.marketCycle = false;\n    },\n    showCompanyData() {\n      this.subMenuVisible.companyData = true;\n    },\n    hideCompanyData() {\n      this.subMenuVisible.companyData = false;\n    },\n    showEvaluation() {\n      this.subMenuVisible.evaluation = true;\n    },\n    hideEvaluation() {\n      this.subMenuVisible.evaluation = false;\n    }\n  }\n};","map":{"version":3,"names":["useUserStore","setup","userStore","userInfo","teamMembers","id","position","email","name","school","class","studentId","subMenuVisible","marketCycle","companyData","evaluation","cyclesToDisplay","methods","showCycles","hideCycles","showCompanyData","hideCompanyData","showEvaluation","hideEvaluation"],"sources":["D:\\Bridge1\\src\\components\\DecisionSimulation.vue"],"sourcesContent":["<template>\n  <div class=\"full\">\n    <nav class=\"navbar  sticky-top navbar-expand-lg navbar-dark bg-dark\">\n      <div class=\"container-fluid\">\n        <router-link class=\"navbar-brand\" to=\"/\">BizWar</router-link>\n        <div class=\"collapse navbar-collapse\">\n          <ul class=\"navbar-nav me-auto\">\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" to=\"/decision\">决策仿真</router-link>\n            </li>\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" to=\"/ImportantInformation\">重要信息</router-link>\n            </li>\n            <li class=\"nav-item\">\n              第<span class=\"text-danger\">{{ userInfo?.group }}</span>组 \n              第<span class=\"text-danger\">{{ userInfo?.number }}</span>企业 \n              <span class=\"text-danger\">{{ userInfo?.rival }}</span>位竞争对手 \n              第<span class=\"text-danger\">{{ userInfo?.cycle }}</span>周期\n            </li>\n            <li class=\"nav-item\">\n              <button @click=\"reset\" class=\"btn btn-danger\">\n                新的一轮\n              </button>\n            </li>\n          </ul>\n          <ul class=\"navbar-nav\">\n            <li class=\"nav-item\">注销</li>\n            <li class=\"nav-item\">我的资料</li>\n          </ul>\n        </div>\n      </div>\n    </nav>\n\n    <div class=\"container-fluid\">\n      <div class=\"row h-100\">\n        <div class=\"col-md-3 sidebar bg-light\">\n          <div class=\"list-group\">\n            <div class=\"list-group-item active\">进行决策</div>\n            <div class=\"list-group-item\">\n              数据报表\n              <div>\n                <a @mouseover=\"showCycles\" @mouseleave=\"hideCycles\">市场周期形势</a>\n                <div v-if=\"subMenuVisible.marketCycle\">\n                  <router-link \n                    v-for=\"cycle in cyclesToDisplay\" \n                    :key=\"cycle\" \n                    :to=\"`/market/cycle/${cycle}`\"\n                  >\n                    第{{ cycle }}周期\n                  </router-link>\n                </div>\n                <a @mouseover=\"showCompanyData\" @mouseleave=\"hideCompanyData\">企业数据报表</a>\n              </div>\n            </div>\n            <div class=\"list-group-item\">竞争结果报表</div>\n            <div class=\"list-group-item\">\n              <a @mouseover=\"showEvaluation\" @mouseleave=\"hideEvaluation\">决策评价总表</a>\n              <div v-if=\"subMenuVisible.evaluation\">\n                <router-link to=\"/evaluation/market\">市场类指标</router-link>\n                <router-link to=\"/evaluation/production\">生产类指标</router-link>\n                <router-link to=\"/evaluation/finance\">财务类指标</router-link>\n                <router-link to=\"/evaluation/overall\">决策综合评价</router-link>\n                <router-link to=\"/evaluation/weight\">评价指标权重</router-link>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"col-md-9\">\n          <div class=\"panel panel-default mt-3\">\n            <div class=\"panel-heading\">\n              <h3 class=\"panel-title\">我的团队</h3>\n            </div>\n            <div class=\"panel-body\">\n              <h4>我建立了团队：<span class=\"text-danger\">11</span> \n                <router-link to=\"/teams/edit/955192\" class=\"btn btn-primary\">修改</router-link>\n              </h4>\n              <table class=\"table table-striped\">\n                <thead>\n                  <tr>\n                    <th>职位</th>\n                    <th>账号</th>\n                    <th>姓名</th>\n                    <th>学校</th>\n                    <th>班级</th>\n                    <th>学号</th>\n                    <th>操作</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"member in teamMembers\" :key=\"member.id\">\n                    <td>{{ member.position }}</td>\n                    <td>{{ member.email }}</td>\n                    <td>{{ member.name }}</td>\n                    <td>{{ member.school }}</td>\n                    <td>{{ member.class }}</td>\n                    <td>{{ member.studentId }}</td>\n                    <td><a href=\"#\" @click.prevent=\"toggleInviteForm(member.id)\">变更</a></td>\n                  </tr>\n                </tbody>\n              </table>\n              <p>您还没有加入任何团队，要加入团队请联系团队CEO。</p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useUserStore } from '@/store/user';\n\nexport default {\n  setup() {\n    const userStore = useUserStore();\n    const userInfo = userStore.userInfo;\n\n    return {\n      userInfo,\n      teamMembers: [\n        {\n          id: 1,\n          position: '总经理',\n          email: '2290140253@qq.com',\n          name: '李清逸',\n          school: '东华大学',\n          class: '信管2201',\n          studentId: '220750204',\n        },\n        // 其他成员\n      ],\n      subMenuVisible: {\n        marketCycle: false,\n        companyData: false,\n        evaluation: false,\n      },\n      cyclesToDisplay: [],\n    };\n  },\n  methods: {\n    showCycles() {\n      this.subMenuVisible.marketCycle = true;\n    },\n    hideCycles() {\n      this.subMenuVisible.marketCycle = false;\n    },\n    showCompanyData() {\n      this.subMenuVisible.companyData = true;\n    },\n    hideCompanyData() {\n      this.subMenuVisible.companyData = false;\n    },\n    showEvaluation() {\n      this.subMenuVisible.evaluation = true;\n    },\n    hideEvaluation() {\n      this.subMenuVisible.evaluation = false; \n    },\n\n  }\n};\n</script>\n\n<style scoped>\nhtml, body {\n  height: 100%;\n  width: 100%;\n  margin: 0;\n  padding: 0;\n}\n\n.full {\n  height: 100vh;\n  width: 100vw;\n}\n\n.sidebar {\n  height: 100%; \n  padding: 20px;\n}\n\n.list-group-item.active {\n  background-color: #0056b3;\n  color: white;\n}\n\n.panel {\n  margin-top: 20px;\n  border-radius: 8px;\n}\n\n.panel-heading {\n  background-color: #f7f7f9;\n  padding: 15px;\n  font-size: 1.25rem;\n}\n\n.panel-body {\n  padding: 20px;\n}\n\n.table-striped > tbody > tr:nth-child(odd) {\n  background-color: #f9f9f9;\n  border-radius: 5px;\n}\n\n.btn {\n  margin-top: 10px;\n}\n\nh4 {\n  font-size: 1.2rem\n}\n</style>\n"],"mappings":"AA+GA,SAASA,YAAW,QAAS,cAAc;AAE3C,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIF,YAAY,CAAC,CAAC;IAChC,MAAMG,QAAO,GAAID,SAAS,CAACC,QAAQ;IAEnC,OAAO;MACLA,QAAQ;MACRC,WAAW,EAAE,CACX;QACEC,EAAE,EAAE,